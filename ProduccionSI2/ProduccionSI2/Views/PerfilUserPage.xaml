<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ProduccionSI2.Views.PerfilUserPage"
    BindingContext="{Binding Main, Source={StaticResource Locator}}"
    Title="Perfil User">
    <ContentPage.Content>
        <StackLayout
            BindingContext="{Binding PerfilUser}"
            Padding="5">
            <Label 
                BindingContext="{Binding Label1,Mode=TwoWay}"
                FontSize="Large"
                HorizontalOptions="CenterAndExpand"
                Text="Ahora estas en el perfil de usuarios"
                VerticalOptions="CenterAndExpand">
            </Label>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>